    "grunt-remove-logging": "^0.2.0",
    "grunt-rename-util": "^1.0.0",
    "grunt-sass": "^2.0.0",
    "grunt-text-replace": "^0.4.0",
    "husky": "^0.14.3",
    "jasmine": "^2.6.0",
    "jasmine-jquery": "^2.1.1",
    "jquery": "^3.2.1",
    "lint-staged": "^7.0.4",
    "node-archiver": "^0.3.0",
    "node-sass": "^4.7.2",
    "phantomjs-prebuilt": "^2.1.14",
    "prettier": "^1.12.1"
